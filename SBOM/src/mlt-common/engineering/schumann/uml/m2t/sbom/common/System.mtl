[module System(
	'http://schumann.engineering/sbom/1'
)/]


[import engineering::schumann::uml::m2t::sbom::common::Component /]


[query public allComponents(
	this : System
) : Set(Component) =
	Set(Namespace) {}
	->union(ownedComponent)
	->union(ownedComponent.allComponents())
	->filter(Component)
	->asSet()
/]