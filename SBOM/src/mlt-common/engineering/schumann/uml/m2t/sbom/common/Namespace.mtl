[module Namespace(
	'http://schumann.engineering/sbom/1'
)/]


[query public sbom_hasSbom(
	this : Namespace
) : Boolean =
	sbom_sbom() <> null
/]


[query public sbom_sbom(
	this : Namespace
) : Sbom =
	if (this = null) then
		null
	else if (oclIsTypeOf(System)) then
		oclAsType(System).sbom
	else if (oclIsTypeOf(Component)) then
		oclAsType(Component).owner.sbom_sbom()
	else
		null
	endif
	endif
	endif
/]