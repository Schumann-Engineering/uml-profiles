[module VersionService(
	'http://www.eclipse.org/uml2/5.0.0/UML'
)/]


[query public isEqualOrNewer(
	needle 		: String, 
	haystack 	: String
) : Boolean = 
	isEqual(haystack) or
	isNewer(haystack)
/]


[query public isEqualOrOlder(
	needle 		: String, 
	haystack 	: String
) : Boolean = 
	isEqual(haystack) or
	isOlder(haystack)
/]


[query public isEqual(
	needle : String, 
	haystack : String
) : Boolean = 
	invoke(
		'engineering.schumann.uml.m2t.common.services.VersionServiceImpl',
		'isEqual(java.lang.String, java.lang.String)',
		Sequence{
			needle,
			haystack
	})
/]


[query public isNewer(
	needle : String, 
	haystack : String
) : Boolean = 
	invoke(
		'engineering.schumann.uml.m2t.common.services.VersionServiceImpl',
		'isNewer(java.lang.String, java.lang.String)',
		Sequence{
			needle,
			haystack
	})
/]


[query public isOlder(
	needle : String, 
	haystack : String
) : Boolean = 
	invoke(
		'engineering.schumann.uml.m2t.common.services.VersionServiceImpl',
		'isOlder(java.lang.String, java.lang.String)',
		Sequence{
			needle,
			haystack
	})
/]


[query public compareVersions(
	needle : String, 
	haystack : String
) : Integer = 
	invoke(
		'engineering.schumann.uml.m2t.common.services.VersionServiceImpl',
		'compareVersions(java.lang.String, java.lang.String)',
		Sequence{
			needle,
			haystack
	})
/]
