[comment encoding = UTF-8 /]
[module Model('http://www.eclipse.org/uml2/5.0.0/UML')/]

[import engineering::schumann::uml::m2t::Element /]

[query public _getAllStereotypedElements(
	model : Model,
	stereotypeName : String
) : Set(Element) =
	model
	.allOwnedElements()
	->select( element : Element | element._hasStereotype(stereotypeName))
/]