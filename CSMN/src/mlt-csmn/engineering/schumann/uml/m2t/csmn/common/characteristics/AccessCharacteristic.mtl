[comment encoding = UTF-8 /]
[module AccessCharacteristic(
	'http://www.eclipse.org/uml2/5.0.0/UML',
	'http://schumann.engineering/csmn/1'
)]


[import engineering::schumann::uml::m2t::Element /]


[query public csmn_hasAccessCharacteristic(
	this : NamedElement
) : Boolean =
	this
	._hasStereotype('CSMN::AccessCharacteristic')
/]


[query public csmn_accessCharacteristic(
	this : NamedElement
) : AccessCharacteristic =
	this
	._getStereotypeInstance('CSMN::AccessCharacteristic')
/]