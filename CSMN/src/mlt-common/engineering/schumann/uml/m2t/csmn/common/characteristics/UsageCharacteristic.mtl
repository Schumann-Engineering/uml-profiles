[comment encoding = UTF-8 /]
[module UsageCharacteristic(
	'http://www.eclipse.org/uml2/5.0.0/UML',
	'http://schumann.engineering/csmn/1'
)]


[import engineering::schumann::uml::m2t::uml::common::Element /]


[query public csmn_hasUsageCharacteristic(
	this : NamedElement
) : Boolean =
	this
	._hasStereotype('CSMN::UsageCharacteristic')
/]


[query public csmn_usageCharacteristic(
	this : NamedElement
) : UsageCharacteristic =
	this
	._getStereotypeInstance('CSMN::UsageCharacteristic')
/]