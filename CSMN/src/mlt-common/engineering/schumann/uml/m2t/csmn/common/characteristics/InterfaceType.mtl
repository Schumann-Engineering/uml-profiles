	endif
	endif
/]


[query public csmn_isEnergyInterface(
	this : InterfaceType
) : Boolean =
	this.csmn_interfaceTypeCategory() = 'energy'
/]


[query public csmn_isNetworkInterface(
	this : InterfaceType
) : Boolean =
	this.csmn_interfaceTypeCategory() = 'network'
/]


[query public csmn_isNonNetworkInterface(
	this : InterfaceType
) : Boolean =
	this.csmn_interfaceTypeCategory() = 'non-network'
/]


[query public csmn_isMaterialInterface(
	this : InterfaceType
) : Boolean =
	this.csmn_interfaceTypeCategory() = 'material'
/]


[query public csmn_isStorageInterface(
	this : InterfaceType
) : Boolean =
	this.csmn_interfaceTypeCategory() = 'storage'
/]


[query public csmn_isOtherInterface(
	this : InterfaceType
) : Boolean =
	this.csmn_interfaceTypeCategory() = '?'
/]


[query public csmn_isWirelessInterface(
	this : InterfaceType
) : Boolean =
	let list : Set(InterfaceType) = Set{
		InterfaceType::Bluetooth,
		InterfaceType::Cellular,
		InterfaceType::Cellular2G,
		InterfaceType::Cellular3G,
		InterfaceType::Cellular4G,
		InterfaceType::Cellular5G,
		InterfaceType::Nfc,
		InterfaceType::WifiAccessPoint,
		InterfaceType::WifiClient,
		InterfaceType::Zigbee
	}
	in
		list->includes(this)
/]


[query public csmn_toString(
	this : InterfaceType
) : String =
	     if (this = InterfaceType::AirByFan) 		then 'Air by Fan'
	else if (this = InterfaceType::AirByTurbine)	then 'Air by Turbine'
	else if (this = InterfaceType::Cellular2G)		then 'Cellular (2G)'
	else if (this = InterfaceType::Cellular3G)		then 'Cellular (3G)'
	else if (this = InterfaceType::Cellular4G)		then 'Cellular (4G)'
	else if (this = InterfaceType::Cellular5G)		then 'Cellular (5G)'
	else if (this = InterfaceType::Display)			then 'Display'
	else if (this = InterfaceType::DisplaySmall)	then 'Display'
	else if (this = InterfaceType::Hdmi)			then 'HDMI'
	else if (this = InterfaceType::Nfc)				then 'NFC'
	else if (this = InterfaceType::SdCard)			then 'SD card'
	else if (this = InterfaceType::Usb)				then 'USB'
	else if (this = InterfaceType::UsbA)			then 'USB A'
	else if (this = InterfaceType::UsbC)			then 'USB C'
	else if (this = InterfaceType::UsbFlash)		then 'USB mass storage device'
	else if (this = InterfaceType::WifiAccessPoint)	then 'Wifi Access Point'
	else if (this = InterfaceType::WifiClient)		then 'Wifi Client'
	else this.toString()
	endif
	endif
